<%= render partial: 'spree/admin/users/tabs', locals: { current: :device_token } %>

<div data-hook="admin_user_device_token_new_form_header">
  <%= render partial: 'spree/admin/shared/error_messages', locals: { target: @object } %>
</div>
<div data-hook="admin_user_device_token_new_form">
  <%= form_with model: @object, url: { action: 'create' } do |f| %>
    <%= render partial: 'form', locals: { f: f } %>
    <div data-hook="admin_user_device_token_new_form_buttons">
      <%= render partial: 'spree/admin/shared/new_resource_links' %>
    </div>
  <% end %>
</div>
