<% content_for :page_title do %>
<%= link_to plural_resource_name(SpreeCmCommissioner::Guest), spree.admin_order_guests_url(@order) %> /
  <%= Spree.t(:guest_infomation) %>
<% end %>

<%= form_with model: @object, url: { action: 'create' }, html: { multipart: true } do |f| %>
  <fieldset>
    <%= f.hidden_field :line_item_id, value: params[:line_item_id] %>
    <%= render partial: 'form', locals: { f: f } %>
    <%= render partial: 'spree/admin/shared/new_resource_links' %>
  </fieldset>
<% end %>
