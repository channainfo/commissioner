<%= render partial: 'spree/admin/shared/product_tabs', locals: { current: :pricing_rates } %>
<%= render partial: 'spree/admin/shared/pricing_rate_sidebar', locals: { current: :edit, pricing_rate: @object } %>
<%= render partial: 'spree/admin/shared/error_messages', locals: { target: @object } %>

<%= form_with model: @object, url: { action: :update } do |f| %>
  <fieldset class="card card-body">
    <%= render partial: 'form', locals: { f: f } %>
    <%= render partial: 'spree/admin/shared/edit_resource_links' %>
  </fieldset>
<% end %>
