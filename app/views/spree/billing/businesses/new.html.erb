<% content_for :page_title do %>
  <%= page_header_back_button spree.billing_businesses_url %>
  <%= @businesses_taxonomy.name %> /  <%= Spree.t(:new_business) %>
<% end %>

<div data-hook="admin_user_new_form_header">
  <%= render partial: 'spree/admin/shared/error_messages', locals: { target: @object } %>
</div>

<%= form_with model: @object, url: { action: 'create' } do |f| %>
    <%= render partial: 'form', locals: { f: f } %>
    <div data-hook="admin_user_new_form_buttons">
      <%= render partial: 'spree/admin/shared/new_resource_links', locals: { collection_url: spree.billing_businesses_url }   %>
    </div>
  <% end %>
</div>